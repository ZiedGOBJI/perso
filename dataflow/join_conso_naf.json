{
	"name": "join_conso_naf",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "conso_short",
						"type": "DatasetReference"
					},
					"name": "conso"
				},
				{
					"dataset": {
						"referenceName": "naf_code",
						"type": "DatasetReference"
					},
					"name": "naf"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "join_result",
						"type": "DatasetReference"
					},
					"name": "join"
				}
			],
			"transformations": [
				{
					"name": "join1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          operateur as string,",
				"          annee as string,",
				"          filiere as string,",
				"          code_categorie_consommation as string,",
				"          libelle_categorie_consommation as string,",
				"          code_grand_secteur as string,",
				"          libelle_grand_secteur as string,",
				"          code_naf as string,",
				"          libelle_secteur_naf2 as string,",
				"          conso as string,",
				"          pdl as string,",
				"          indqual as string,",
				"          nombre_mailles_secretisees as string,",
				"          code_region as string,",
				"          libelle_region as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> conso",
				"source(output(",
				"          ligne as string,",
				"          code as string,",
				"          int_v2_40c as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> naf",
				"conso, naf join(code_naf == code,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1",
				"join1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          {operateur;annee;filiere;code_categorie_consommation;libelle_categorie_consommation;code_grand_secteur;libelle_grand_secteur;code_naf;libelle_secteur_naf2;conso;pdl;indqual;nombre_mailles_secretisees;code_region;libelle_region} as string",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> join"
			]
		}
	}
}